<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="RangeSeekBar">
        <attr name="absoluteMinValue" format="integer|float"/>
        <attr name="absoluteMaxValue" format="integer|float"/>
        <attr name="singleThumb" format="boolean"/>
        <attr name="lineHeight" format="dimension"/>
        <attr name="backgroundLineColor" format="color|reference"/>
        <attr name="highlightedLineColor" format="color|reference"/>
        <attr name="android:textColor"/>
    </declare-styleable>
    <declare-styleable name="SplashView">
        <attr name="strokeWidth" format="float"/>
        <attr name="strokeColor" format="color|reference"/>
        <attr name="phase" format="float"/>
        <attr name="duration" format="integer"/>
        <attr name="fadeFactor" format="float"/>
    </declare-styleable>
</resources>